<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cldla.finalproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_mountain"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_mountain"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".JoinActivity" />
        <activity android:name=".WriteReviewActivity" />
        <activity android:name=".ReviewListActivity" />
        <activity android:name=".SeeReviewActivity"></activity>
        <activity
            android:name=".MountPage"
            android:label="@string/title_activity_mount_page"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>